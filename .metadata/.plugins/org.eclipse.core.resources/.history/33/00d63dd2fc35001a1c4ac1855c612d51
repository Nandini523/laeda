package thread;

 class Mthread extends Thread {
@Override
public void run() {
	// TODO Auto-generated method stub
	super.run();
	for(int i=0;i<=10;i++)
		{ 
		Mthread.sleep(1000);
		System.out.println(i);}
}
}
public class Mythread
{
	public static void main(String [] ar)
	{
		Mthread m=new Mthread();
		m.start();
	
		}
		
	}
	}